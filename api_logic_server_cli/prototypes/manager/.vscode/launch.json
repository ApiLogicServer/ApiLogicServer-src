{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "GENAI",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/venv/lib/python3.12/site-packages/api_logic_server_cli/cli.py",
            "redirectOutput": true,
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "", 
                "SECURITY_ENABLED": "False", 
                "PYTHONHASHSEED": "0", 
                "APILOGICSERVER_DEBUG": "False",
                "OPT_LOCKING": "optional"},
            "justMyCode": false,
            "args": [ "genai", "--retries=-1",
                "--using=${workspaceFolder}/system/genai/examples/${input:inputGenaiProjectName}"],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        // just add the next prompt file to the system/genai/temp/>project> directory
        {
            "name": "  - GENAI - Iterate",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/venv/lib/python3.12/site-packages/api_logic_server_cli/cli.py",
            "redirectOutput": true,
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "", 
                "SECURITY_ENABLED": "False", 
                "PYTHONHASHSEED": "0", 
                "APILOGICSERVER_DEBUG": "False",
                "OPT_LOCKING": "optional"},
            "justMyCode": false,
            "args": [ "genai",   
                    "--using=system/genai/temp/${input:inputGenaiProjectName}" ],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "  - GENAI - Retry from repaired chatgpt response",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/venv/lib/python3.12/site-packages/api_logic_server_cli/cli.py",
            "redirectOutput": true,
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "", 
                "SECURITY_ENABLED": "False", 
                "PYTHONHASHSEED": "0", 
                "APILOGICSERVER_DEBUG": "False",
                "OPT_LOCKING": "optional"},
            "justMyCode": false,
            "args": [ "genai",
                "--using=${input:inputGenaiProjectName}",
                "--gen-using-file=system/genai/temp/chatgpt_retry.response"],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "  - GENAI - Retry from repaired model",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/venv/lib/python3.12/site-packages/api_logic_server_cli/cli.py",
            "redirectOutput": true,
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "", 
                "SECURITY_ENABLED": "False", 
                "PYTHONHASHSEED": "0", 
                "APILOGICSERVER_DEBUG": "False",
                "OPT_LOCKING": "optional"},
            "justMyCode": false,
            "args": [ "create",
                "--project-name=genai_demo",
                "--db_url=sqlite",
                "--from-model=system/genai/temp/create_db_models.py"],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "  - GENAI - Postgresql",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/venv/lib/python3.12/site-packages/api_logic_server_cli/cli.py",
            "redirectOutput": true,
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "", 
                "SECURITY_ENABLED": "False", 
                "PYTHONHASHSEED": "0", 
                "APILOGICSERVER_DEBUG": "False",
                "OPT_LOCKING": "optional"},
            "justMyCode": false,
            "args": [ "genai",
                "--db-url=postgresql://postgres:p@localhost/genai_demo",
                "--using=${input:inputGenaiProjectName}_pg"],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "ApiLogicServer Create",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/venv/lib/python3.12/site-packages/api_logic_server_cli/cli.py",
            "redirectOutput": true,
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "", 
                "SECURITY_ENABLED": "False", 
                "PYTHONHASHSEED": "0", 
                "APILOGICSERVER_DEBUG": "False",
                "OPT_LOCKING": "optional"},
            "justMyCode": false,
            "args": [ "create",
                "--project-name=${input:inputProjectName}",
                "--db-url=${input:inputDBURL}"],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "  - API Logic Server Run (run project from manager)",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/${input:runProjectName}",
            "env": {
                "PYTHONHASHSEED": "0", 
                "APILOGICSERVER_DEBUG": "False",
                "OPT_LOCKING": "optional"},
            "justMyCode": false,
            "program": "api_logic_server_run.py",
            "redirectOutput": true,
            "console": "integratedTerminal"
        },
        {
            "name": "ApiLogicServer Create nw_sample_nocust",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/venv/lib/python3.12/site-packages/api_logic_server_cli/cli.py",
            "redirectOutput": true,
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "", 
                "PYTHONHASHSEED": "0", 
                "APILOGICSERVER_DEBUG": "True",
                "OPT_LOCKING": "optional"},
            "justMyCode": false,
            "args": [ "create",
                "--project-name=samples/nw_sample_nocust",
                "--db-url=nw"],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "ApiLogicServer Create nw_sample With Customizations",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/venv/lib/python3.12/site-packages/api_logic_server_cli/cli.py",
            "redirectOutput": true,
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "", 
                "PYTHONHASHSEED": "0", 
                "APILOGICSERVER_DEBUG": "True",
                "OPT_LOCKING": "optional"},
            "justMyCode": false,
            "args": [ "create",
                "--project-name=samples/nw_sample",
                "--db-url=nw+"],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "App Model Editor - Prepare",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}/system/app_model_editor/ui/yaml",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["install"],
            "console": "integratedTerminal"
        },
        {
            "name": "  - Start Server",
            "type": "debugpy",
            "cwd": "${workspaceFolder}/system/app_model_editor",
            "env": {
                "PYTHONPATH": "", 
                "PYTHONHASHSEED": "0", 
                "APILOGICPROJECT_DEBUG": "False"},
            "request": "launch",
            "program": "api_logic_server_run.py",
            "redirectOutput": true,
            "justMyCode": false,
            "args": ["--flask_host=localhost", "--port=5655", "--swagger_port=5655", "--swagger_host=localhost", "--verbose=False"],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "  - Start",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}/system/app_model_editor/ui/yaml",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["start"],
            "port": 4299,
            "restart": true,
            "timeout": 30000,
            "protocol": "inspector",
            "justMyCode": false,
            "console": "integratedTerminal",
            "env": {
                "NODE_ENV": "development"
            }
        },
        {
            "name": "Python: Module",
            "type": "debugpy",
            "request": "launch",
            "redirectOutput": true,
            "justMyCode": false,            
            "module": "enter-your-module-name"
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "redirectOutput": true,
            "justMyCode": false,            
            "console": "internalConsole"
        },
        {
            "name": "Sys Info",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "samples/nw_sample/venv_setup/py.py",
            "redirectOutput": true,
            "args": ["sys-info"],
            "justMyCode": false,
            "console": "integratedTerminal"
        }
    ],
    "inputs": [
        {
            "id": "inputProjectName",
            "type": "promptString",
            "description": "Project Name (will create directory with this name)",
            "default": "nw"
        },
        {
            "id": "runProjectName",
            "type": "promptString",
            "description": "Project Name (folder) to run",
            "default": "samples/nw"
        },
        {
            "id": "inputGenaiProjectName",
            "type": "promptString",
            "description": "Project Name (will create directory with this name)",
            "default": "genai_demo"
        },
        {
            "id": "inputDBURL",
            "type": "promptString",
            "description": "Database URL (SQLAlchemy URI)",
            "default": "sqlite:///nw.sqlite"
        }
    ]
  }
