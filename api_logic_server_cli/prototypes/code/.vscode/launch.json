{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "ApiLogicServer Create a Project",
            "type": "debugpy",
            "request": "launch",
            "program": "cli_path",
            "redirectOutput": true,
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "", 
                "SECURITY_ENABLED": "False", 
                "PYTHONHASHSEED": "0", 
                "APILOGICSERVER_DEBUG": "False",
                "OPT_LOCKING": "optional"},
            "justMyCode": false,
            "args": [ "create",
                "--project-name=${input:inputProjectName}",
                "--db-url=${input:inputDBURL}"],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "ApiLogicServer Create Tutorial",
            "type": "debugpy",
            "request": "launch",
            "program": "cli_path",
            "redirectOutput": true,
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "", 
                "PYTHONHASHSEED": "0", 
                "APILOGICSERVER_DEBUG": "True",
                "OPT_LOCKING": "optional"},
            "justMyCode": false,
            "args": [ "create",
                "--project-name=ApiLogicProject",
                "--db-url=nw"],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "ApiLogicServer Create Tutorial With Customizations",
            "type": "debugpy",
            "request": "launch",
            "program": "cli_path",
            "redirectOutput": true,
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "", 
                "PYTHONHASHSEED": "0", 
                "APILOGICSERVER_DEBUG": "True",
                "OPT_LOCKING": "optional"},
            "justMyCode": false,
            "args": [ "create",
                "--project-name=ApiLogicProject_customized",
                "--db-url=nw+"],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Python: Module",
            "type": "debugpy",
            "request": "launch",
            "redirectOutput": true,
            "justMyCode": false,            
            "module": "enter-your-module-name"
        },
        {
            "name": "Python: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "redirectOutput": true,
            "justMyCode": false,            
            "console": "internalConsole"
        },
        {
            "name": "Sys Info",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "program": "venv_setup/py.py",
            "redirectOutput": true,
            "args": ["sys-info"],
            "justMyCode": false,
            "console": "integratedTerminal"
        }
    ],
    "inputs": [
        {
            "id": "inputProjectName",
            "type": "promptString",
            "description": "Project Name (will create directory with this name)",
            "default": "sample_ai"
        },
        {
            "id": "inputDBURL",
            "type": "promptString",
            "description": "Database URL (SQLAlchemy URI)",
            "default": "sqlite:///sample_ai.sqlite"
        }
    ]
  }
  